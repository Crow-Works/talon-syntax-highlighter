{
  "name": "talon-syntax-highlighter",
  "displayName": "Talon-Syntax-Highlighter",
  "description": "Syntax highlighter for the .tln files used by the Talon build system",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "tln",
        "aliases": [
          "Talon",
          "tln"
        ],
        "extensions": [
          ".tln"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tln",
        "scopeName": "source.tln",
        "path": "./syntaxes/tln.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vscode": "^1.1.37",
    "vscode-oniguruma": "^2.0.1",
    "vscode-textmate": "^9.0.0",
    "vscode-tmgrammar-test": "^0.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.25"
  }
}
